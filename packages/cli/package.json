{
  "name": "@cryptoeconomicslab/tezos-liteclient-cli",
  "version": "0.0.9",
  "description": "CLI for tezos wallet",
  "author": {
    "name": "Cryptoeconomics Lab",
    "url": "https://www.cryptoeconomicslab.com/"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "build:check": "tsc --noEmit",
